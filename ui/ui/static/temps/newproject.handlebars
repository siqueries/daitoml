<div style="margin-top:50px;"></div>

<div class="row">
    <div class="col-md-4 side-nav"></div>
    <div class="col-md-5">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title">Create a machine learning project.</h3>
            </div>
            <div class="panel-body">

                <form method="post" style="max-width: 412px;" id="profileForm">

                    <span style="margin-left:40px;">

                    </span>
                    <div class="form-group">
                        <label for="name" class="control-label">Project Name:</label>
                        <input type="text" id="name" name="name" class="form-control" value="">
                    </div>
                    <div class="form-group">
                        <label for="description" class="control-label">Description:</label>
                        <textarea rows="5" cols="10" id="description" name="description" class="form-control"></textarea>
                    </div>
                    <div class="form-atfooter">
                        <button type="submit" class="btn btn-primary" id="create-project">Create Project</button>
                        <button type="submit" class="btn btn-default" id="cancel">Cancel</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <div class="col-md-4 side-nav"></div>
</div>